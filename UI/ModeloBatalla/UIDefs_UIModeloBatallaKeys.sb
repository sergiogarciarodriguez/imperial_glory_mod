  # Las teclas de control de camara estaran en el sb UIDefs_CtrlMovCamara.sb

  
  # Tecla para ocultar el UI al completo
  [ .TIPO UIC_TECLA         .ID 'KDOC'    .PARAMS [ .TECLA KEY_I .MKEYS [ L_CTRL ] .EVENTOS [ KEY_DOWN KEY_UP ] ] ]  
  [ .TIPO UIC_TECLA         .ID 'KDOC'    .PARAMS [ .TECLA KEY_I .MKEYS [ R_CTRL ] .EVENTOS [ KEY_DOWN KEY_UP ] ] ]  
       
  # Tecla para activar el menu de juego
  [ .TIPO UIC_TECLA         .ID 'MESC'    .PARAMS [ .TECLA KEY_ESCAPE .MKEYS [ ] .EVENTOS [ KEY_DOWN KEY_UP ] ] ]

  # Activacion / Desactivacion del mini mapa
  [ .TIPO UIC_TECLA .ID 'KMAP' .PARAMS [ .TECLA KEY_M .MKEYS [ ] .EVENTOS [ KEY_DOWN KEY_UP ] ] ]
  
  # Boton de orientar
  [ .TIPO UIC_TECLA .ID 'KORI' .PARAMS [ .TECLA KEY_N .MKEYS [ ] .EVENTOS [ KEY_DOWN KEY_UP ] ] ]

  # Cambio del cursor de modo de ataque y cambio de angulo de tropa junto al boton central  
  # toda esta movidilla extraña es para q se reciba el evento en caso de q se pulse CTRL + SHIFT
  [ .TIPO UIC_TECLA  .ID 'K_CA' .PARAMS [ .TECLA KEY_LSHIFT .MKEYS [ L_SHIFT ] .EVENTOS [ KEY_DOWN KEY_UP ] ] ]
