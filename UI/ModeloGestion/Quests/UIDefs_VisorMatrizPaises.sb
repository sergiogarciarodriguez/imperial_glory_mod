.BANDERA_JUG_PRUSIA_ENABLE     [ .FILE "Game\UI\ModeloGestion\IconosQuest.tga" .DX 2 .DY 8  .SX 33 .SY 18 .U 145 .V 505 .ALPHA TRUE ]      
