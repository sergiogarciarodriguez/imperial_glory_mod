.BANDERA_JUG_GRAN_BRETANYA_DISABLE    [ .FILE "Game\UI\ModeloGestion\IconosQuest.tga" .DX 2 .DY 8  .SX 33 .SY 18 .U 37 .V 551 .ALPHA TRUE]      

.BANDERA_JUG_AUSTRIA_ENABLE     [ .FILE "Game\UI\ModeloGestion\IconosQuest.tga" .DX 2 .DY 8  .SX 33 .SY 18 .U 73 .V 505 .ALPHA TRUE ]      
