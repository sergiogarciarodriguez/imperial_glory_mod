###############################################################################
# Mostrará los paises que en la actualidad estan realizando un quest
###############################################################################

.PARAMS
[
  .CENTER_ICONS FALSE

  # Definimos la matriz de vision
  .MATRIZ
  [
   .FILAS 5
   .COLUMNAS 1
  ]     
  
  .DRAW_PARAMS 
  [ 
    .X 411 .Y 565
    .CELL_XSPACE 0 
    .CELL_YSPACE 0 
    .SLIDER_XSPACE 10 
    .OBJ_WIDTH 200
    .OBJ_HEIGHT 31
  ]
  
  .PRIORITY_BASE 254  
]  

#
# Iconos con las banderas de los paises para poner si estan investigando una quest
.BANDERA_JUG_FRANCIA_ENABLE     [ .FILE "Game\UI\ModeloGestion\IconosQuest.tga" .DX 2 .DY 8 .SX 33 .SY 18 .U 1 .V 505 .ALPHA TRUE]      
.BANDERA_JUG_FRANCIA_DISABLE    [ .FILE "Game\UI\ModeloGestion\IconosQuest.tga" .DX 2 .DY 8 .SX 33 .SY 18 .U 1 .V 551 .ALPHA TRUE]      

