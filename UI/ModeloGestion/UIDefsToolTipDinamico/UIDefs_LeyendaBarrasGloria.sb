#
# LEYENDA DE LA BARRA DE PUNTOS DE GLORIA DEL MODO DE ESTADISTICAS
#
#


#TITULO
[
  .TIPO UIC_WRAPTEXT .ID 'WTTI'
  .PARAMS
  [     
    .X 0 .Y 10 .SX 320 .SY 50
    .ALIGN CENTER
    .TEXT
    [
      .FONT "Fonts/Times10.fnt" 
      .COLOR [ .R 255 .G 255 .B 255 ]          
    ]
    .MSG LTEXT_LEYENDA_GLORIA
  ]
]

#COLOR DE LA BARRA DE PUNTOS DE GLORIA
[ .TIPO UIC_BITMAP .ID 'BMPG'
  .PARAMS
  [ .X 30 .Y 45 .U 771 .V 70 .SX 22 .SY 22
    .FILE "Game/UI/ModeloGestion/InterfaceGestionNeutrales.tga" .PRIORITY 255 .ALPHA TRUE  
  ]      
]
   
#TEXTO DE LA BARRA DE PUNTOS DE GLORIA
[
  .TIPO UIC_TEXT .ID 'TXPG' 
  .PARAMS 
  [ .ALIGN LEFT .X 60 .Y 50 .TEXT LTEXT_LEYENDA_BARRA_GLORIA
    .FONT "Fonts/Times9.fnt"  .COLOR [ .R 255 .G 255 .B 255 ]
  ]
] 
 
 