[
  .KEY ALIANZA
  .PARAMS
  [
    .COMPORTAMIENTO FIJO
    .FONDO
    [
      .X 24 .Y 140
      .SX 98 .SY 51
      .ALPHA TRUE
      .DISABLE [ .FILE "Game/UI/ModeloGestion/Diplomacia/PlantillaIconosDiplomacia.tga" .U 1 .V 1 ]
      .SOLTADO [ .FILE "Game/UI/ModeloGestion/Diplomacia/PlantillaIconosDiplomacia.tga" .U 1  .V 105 ]
      .SELECCIONADO [ .FILE "Game/UI/ModeloGestion/Diplomacia/PlantillaIconosDiplomacia.tga" .U 1  .V 209 ]
      .PULSADO [.FILE "Game/UI/ModeloGestion/Diplomacia/PlantillaIconosDiplomacia.tga" .U 1  .V 314 ]
      .PRIORITY 100
    ]
  
    .TOOL_TIP 
    [ 
      .TEXT LTEXT_DIPLOMACIA_ALIANZA 
      %Game/UI/Comun/UIDefs_DefaultToolTip.sb
    ]
    
     # Usamos el cursor de ayuda contextual 
    .ALTERNATE_CURSOR CUR_CONTEXTHELP
  ]       
]

